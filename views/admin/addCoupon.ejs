        <script>
            // Array to store used coupon codes
            const usedCouponCodes = ["EXISTINGCODE123", "EXAMPLECODE456"]; // Example initial codes
    
            function validateForm() {
                const nameInput = document.getElementById('name');
                const couponCodeInput = document.getElementById('couponcode');
                const discountAmountInput = document.getElementById('discountamount');
                const activateDateInput = document.getElementById('activateDate');
                const expiryDateInput = document.getElementById('expiryDate');
                const criteriaAmountInput = document.getElementById('criteriaamount');
    
                const nameError = document.getElementById('nameError');
                const couponCodeError = document.getElementById('couponCodeError');
                const discountAmountError = document.getElementById('discountAmountError');
                const activateDateError = document.getElementById('activateDateError');
                const expiryDateError = document.getElementById('expiryDateError');
                const criteriaAmountError = document.getElementById('criteriaAmountError');
    
                let isValid = true;
    
                // Trim whitespace from the input values
                const name = nameInput.value.trim();
                const couponCode = couponCodeInput.value.trim();
                const discountAmount = discountAmountInput.value.trim();
                const activateDate = activateDateInput.value.trim();
                const expiryDate = expiryDateInput.value.trim();
                const criteriaAmount = criteriaAmountInput.value.trim();
    
                // Validate Name
                if (name === '') {
                    nameError.textContent = 'Name cannot be empty.';
                    isValid = false;
                } else {
                    nameError.textContent = ''; // Clear any previous error message
                }
    
                // Validate Coupon Code
                if (couponCode === '') {
                    couponCodeError.textContent = 'Coupon Code cannot be empty.';
                    isValid = false;
                } else if (usedCouponCodes.includes(couponCode)) {
                    couponCodeError.textContent = 'Coupon Code has already been used.';
                    isValid = false;
                } else {
                    couponCodeError.textContent = ''; // Clear any previous error message
                }
    
                // Validate Discount Amount
                if (discountAmount === '' || isNaN(discountAmount) || discountAmount <= 0) {
                    discountAmountError.textContent = 'Discount Amount must be a positive number.';
                    isValid = false;
                } else {
                    discountAmountError.textContent = ''; // Clear any previous error message
                }
    
                // Validate Activate Date
                if (activateDate === '') {
                    activateDateError.textContent = 'Activate Date cannot be empty.';
                    isValid = false;
                } else {
                    activateDateError.textContent = ''; // Clear any previous error message
                }
    
                // Validate Expiry Date
                if (expiryDate === '') {
                    expiryDateError.textContent = 'Expiry Date cannot be empty.';
                    isValid = false;
                } else if (expiryDate <= activateDate) {
                    expiryDateError.textContent = 'Expiry Date must be after the Activate Date.';
                    isValid = false;
                } else {
                    expiryDateError.textContent = ''; // Clear any previous error message
                }
    
                // Validate Criteria Amount
                if (criteriaAmount === '' || isNaN(criteriaAmount) || criteriaAmount <= 0) {
                    criteriaAmountError.textContent = 'Criteria Amount must be a positive number.';
                    isValid = false;
                } else {
                    criteriaAmountError.textContent = ''; // Clear any previous error message
                }
    
                return isValid; // Return the overall form validity
            }
        </script>
